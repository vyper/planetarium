<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title><%= name %></title>
  <subtitle><%= title %></subtitle>
  <link href="<%= url %>/atom.xml" rel="self" />
  <link href="<%= url %>" />
  <id>urn:uuid:<%= (0..36).to_a.map{|a| rand(36).to_s(36)}.join %></id>
  <updated><%= updated.strftime("%Y-%m-%d %H:%M:%S %Z") %></updated>
  <generator uri="http://github.com/vyper/planetarium/">Planetarium (<%= version %>) - http://github.com/vyper/planetarium/</generator>
 
  <% entries.each do |entry| %>
  <entry>
    <id><%= entry.id %></id>
    <title><%= entry.title %></title>
    <link href="<%= entry.url %>" />
    <% if !entry.updated.nil? %>
    <updated><%= entry.updated.strftime("%Y-%m-%d %H:%M:%S %Z") %></updated>
    <% end %>
    <author>
      <name><%= entry.author %></name>
      <uri><%= entry.feed.url %></uri>
    </author>
    <summary><%= entry.summary %></summary>
    <content type="html"><%= entry.content %></content>
    <source>
      <title type="html"><%= entry.feed.title %></title>
      <link rel="self" href="<%= entry.feed.feed_url %>"/>
      <id><%= entry.feed.feed_url %></id>
      <updated><%= entry.feed.last_modified.strftime("%Y-%m-%d %H:%M:%S %Z") %></updated>
    </source>
  </entry>
  <% end %>
</feed>

